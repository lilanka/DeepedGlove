{
  "name": "w2_opt",
  "n_gpu": 0,

  "arch": {
    "type": "MuZero",
    "args": {}
  },

  "control_params": {
    "n_temperature": 16,
    "n_level": 11,
    "n_motor": 1,
    "flow": 0
  },

  "revloss": "cross_entropy",
  "loss": "mse_loss",
  "metrics": [
    "accuracy", "top_k_acc"
  ],

  "lr_scheduler": {
    "type": "StepLR",
    "args": {
      "step_size": 20,
      "gamma": 0.1
    }
  }, 

  "trainer": {
    "epochs": 16,
    "save_dir": "saved/",
    "save_period": 1,
    "early_stop": 10,
    "tensorboard": true
  }
}
